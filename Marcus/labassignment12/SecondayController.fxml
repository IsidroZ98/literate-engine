<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="your.package.SecondaryController">
    <children>
        <!-- Labels for displaying responses -->
        <Label fx:id="nameLabel" layoutX="32.0" layoutY="39.0" text="Thank you for answering all the questions, <name-response>." />

        <Label fx:id="foodLabel" layoutX="32.0" layoutY="105.0" text="It is true that <favorite-fooditem-response> is a delicious food item." />

        <Label fx:id="mediaLabel" layoutX="32.0" layoutY="171.0" text="Well, guess you better get back on <favorite-social-media-platform-response> now." />

        <!-- Go Back Button -->
        <Button layoutX="32.0" layoutY="237.0" mnemonicParsing="false" onAction="#handleGoBackButton" text="Go Back" />
    </children>
</AnchorPane>
