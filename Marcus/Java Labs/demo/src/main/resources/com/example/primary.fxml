<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="your.package.PrimaryController">
    <children>
        <Label layoutX="34.0" layoutY="34.0" text="What is your name?" />
        <TextField fx:id="nameTextField" layoutX="34.0" layoutY="54.0" prefWidth="200.0" />

        <Label layoutX="34.0" layoutY="90.0" text="Hello, what is your favorite food item?" />
        <TextField fx:id="foodTextField" layoutX="34.0" layoutY="110.0" prefWidth="200.0" />

        <Label layoutX="34.0" layoutY="146.0" text="What is your favorite social media platform?" />
        <TextField fx:id="mediaTextField" layoutX="34.0" layoutY="166.0" prefWidth="200.0" />

        <Button layoutX="34.0" layoutY="210.0" mnemonicParsing="false" onAction="#handleSubmitButton" text="Submit" />
    </children>
</AnchorPane>
